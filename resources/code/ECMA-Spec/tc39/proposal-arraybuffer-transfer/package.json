  },
  "homepage": "https://github.com/tc39/proposal-arraybuffer-transfer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tc39/proposal-arraybuffer-transfer.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@tc39/ecma262-biblio": "^2.1.2632",
    "ecmarkup": "^14.1.3"
  }
}
