    "@octokit/types": "^6.34.0",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "eslint": "^8.13.0",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config"
  },
  "dependencies": {
    "@actions/github": "^5.0.1"
  }
}
